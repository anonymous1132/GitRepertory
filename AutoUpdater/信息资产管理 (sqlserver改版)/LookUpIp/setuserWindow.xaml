<Window x:Class="LookUpIp.setuserWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="设置用户信息" Height="150" Width="200" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner">
    <Grid Background="LightCyan">
        <StackPanel Orientation="Vertical" VerticalAlignment="Center">
            <TextBox Name="textbox" Height="30" Width="140" Margin="0 10 0 0"/>
            <Grid HorizontalAlignment="Center">
                <StackPanel Orientation="Horizontal">
                    <Button Content="确定"  Width="50" Margin="0 10" Click="Button_Click" />
                    <Button Content="取消"  Width="50" Margin="10 10 0 10" Click="Button_Click_1" />
                </StackPanel>
            </Grid>
        </StackPanel>

    </Grid>
</Window>
